x=gets.to_i
y=gets.to_i
z=gets.to_i

if x < 0
	puts "A"
elsif y > 0 && z>0
	puts "B"
elsif y > 0 && z<0
	puts "C"
elsif y < 0 && z>0
 	puts "D"
elsif y < 0 && z<0
	puts "E"
end
