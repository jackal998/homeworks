x=gets.to_i
y=gets.to_i
z=gets.to_i

if x < y
	if y < z
		puts "z=#{z}"
	else
		puts "y=#{y}"
	end
elsif x < z
	puts "z=#{z}"
else
	puts "x=#{x}"
end
# your code goes here

x=gets.to_i
y=gets.to_i
z=gets.to_i

if x < y
	if y < z
		puts "#{z},#{y},#{x}"
	elsif x>z
		puts "#{y},#{x},#{z}"
	else	
		puts "#{y},#{z},#{x}"
	end
elsif x < z
	puts "#{z},#{x},#{y}"
elsif z > y
	puts "#{x},#{z},#{y}"
else
	puts "#{x},#{y},#{z}"
end
# your code goes here

